{
  "name": "rocketact-dev-utils",
  "version": "0.0.1",
  "description": "shared utils for rocketact and it's plugins",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npm test",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdf2e/rocketact-dev-utils.git"
  },
  "author": "loveky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdf2e/rocketact-dev-utils/issues"
  },
  "homepage": "https://github.com/jdf2e/rocketact-dev-utils#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^10.12.2",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "chalk": "^2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
